{
  "disable_ai": true,
  "title_bar": {
    "show_menus": false,
    "show_sign_in": false,
    "show_user_picture": true,
    "show_onboarding_banner": false,
    "show_branch_icon": true,
    "show_branch_name": true
  },
  "max_tabs": 5,
  "search": {
    "button": false
  },
  "git_panel": {
    "collapse_untracked_diff": true,
    "sort_by_path": true,
    "fallback_branch_name": "master",
    "button": false
  },
  "notification_panel": {
    "dock": "right"
  },
  "collaboration_panel": {
    "button": false
  },
  "outline_panel": {
    "indent_size": 25,
    "button": false
  },
  "project_panel": {
    "default_width": 250,
    "entry_spacing": "comfortable",
    "auto_fold_dirs": true,
    "dock": "left"
  },
  "indent_guides": {
    "enabled": true,
    "coloring": "fixed"
  },
  "tabs": {
    "git_status": true,
    "file_icons": true
  },
  "centered_layout": {
    "left_padding": 0.1,
    "right_padding": 0.1
  },
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "inlay_hints": {
    "enabled": true
  },
  "git": {
    "inline_blame": {
      "enabled": true,
      "delay_ms": 500,
      "min_column": 80
    }
  },
  "scrollbar": {
    "show": "auto"
  },
  "vim_mode": true,
  "seed_search_query_from_cursor": "selection",
  "ui_font_family": "Liga SFMono Nerd Font",
  "buffer_font_family": "Liga SFMono Nerd Font",
  "ui_font_size": 15,
  "buffer_font_size": 13,
  "buffer_line_height": "comfortable",
  "base_keymap": "VSCode",
  "relative_line_numbers": true,
  "theme": {
    "mode": "system",
    "light": "Fleet Light",
    "dark": "Fleet Dark"
  },
  "terminal": {
    "dock": "bottom",
    "blinking": "on",
    "copy_on_select": true,
    "env": {
      "EDITOR": "zed --wait"
    },
    "detect_venv": {
      "on": {
        "directories": [".venv", "venv"],
        "activate_script": "default"
      }
    }
  },
  "auto_install_extensions": {
    "dart": true,
    "fleet-themes": true,
    "git-firefly": true,
    "html": true,
    "toml": true
  },
  "file_scan_exclusions": [
    "**/.git",
    "**/.svn",
    "**/.hg",
    "**/CVS",
    "**/.DS_Store",
    "**/Thumbs.db",
    "**/.classpath",
    "**/.settings",
    "**/dist",
    "**/.vscode"
  ],
  "remove_trailing_whitespace_on_save": true,
  "always_treat_brackets_as_autoclosed": true
}
